            <div class="index">
                <i class='bx bx-user-circle'></i>
                <p>Thông tin<br /><b>Tài khoản</b></p>

            </div>

        </div>

    </div>
    <hr />
    <div class="container body-content">
        @RenderBody()
        <hr />

    </div>
    <div></div>

    <footer>
        <div class="infor-help">
            <b>THÔNG TIN HỖ TRỢ</b> <br />
            <a href="#">Chính sách bảo hành</a> <br />
            <a href="#">Chính sách bảo hành iPhone - iPad</a> <br />
            <a href="#">Bảo hành Xiaomi chính hãng</a> <br />
            <a href="Chính sách mua hàng online">Chính sách mua hàng online</a> <br />
            <a href="#">Mua hàng trả góp</a> <br />
            <a href="#">Chính sách bảo mật thông tin khách hàng</a> <br />
        </div>



        <div class="infor-contact">
            <b>THÔNG TIN LIÊN LẠC</b><br />
